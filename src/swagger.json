  },
  "components": {
    "schemas": {
      "UserInput": {
        "type": "object",
        "required": ["name", "email"],
        "properties": {
          "name": {
            "type": "string",
            "default": "John Doe"
          },
          "email": {
            "type": "string",
            "default": "john.d@example.net"
          }
        }
      },
      "ResponseWithMessage": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      }
    },
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "name": "x-api-key",
        "in": "header"
      }
    }
  }
}
